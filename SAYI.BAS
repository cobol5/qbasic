CLS
LOCATE 2, 10: INPUT "KAC SAYI GIRILECEK"; N
CLS
FOR A = 1 TO N
    LOCATE 2, 5: PRINT A; ".SAYIYI GIRIN"
    LOCATE 2, 30: INPUT "", SAYI(A)
NEXT A
FOR A = 1 TO N - 1
FOR B = A + 1 TO N
        IF SAYI(A) > SAYI(B) THEN
           SWAP SAYI(A), SAYI(B)
        END IF
NEXT B, A
PRINT "EN KUCUK SAYI", SAYI(1)
PRINT "EN BUYUK SAYI", SAYI(N)


